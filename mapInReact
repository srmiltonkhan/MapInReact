

function Dtail(props) {
  return <li> â–   { props.name }</li>;
}

function Garage() {
  const footers = ['Brocure', 'Contact', 'Audi'];
  return (
    <>
      <ul>
        {footers.map( (footer)=> <Dtail name={footer} />)}
      </ul>
    </>
  );
}

ReactDOM.render(<Garage />, document.getElementById('root'));
